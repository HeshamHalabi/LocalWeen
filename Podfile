  pod 'Firebase/Storage'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Storage'
  pod 'GooglePlaces'
  pod 'FirebaseUI/Facebook'
  pod 'FirebaseUI/Google'
   
   #Facebook
   pod 'FBSDKLoginKit'

end
