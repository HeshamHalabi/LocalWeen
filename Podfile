# platform :ios, '9.0'

target 'LocalWeen' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LocalWeen
  # Google Pods
   pod 'GoogleMaps'
   pod 'Firebase/Database'
   pod 'Firebase/Storage'
   pod 'Firebase/Core'
   pod 'Firebase/Auth'
   pod 'GoogleSignIn'
   pod 'FirebaseUI/Storage'
   pod 'GooglePlaces'
   pod 'OpenInGoogleMaps'
   
   #Facebook
   pod 'FBSDKLoginKit'

   
   #Cosmos Pod
   pod 'Cosmos', '~> 15.0'
   
   #Agrume
   pod 'Agrume'
end
